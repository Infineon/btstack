      <memberdef kind="function" id="group__wiced__bt__mesh__time__client_1ga662c823f204ba19f2aed787a910343cd" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>wiced_bool_t</type>
        <definition>wiced_bool_t wiced_bt_mesh_model_time_client_message_handler</definition>
        <argsstring>(wiced_bt_mesh_event_t *p_event, uint8_t *p_data, uint16_t data_len)</argsstring>
        <name>wiced_bt_mesh_model_time_client_message_handler</name>
        <param>
          <type><ref refid="structwiced__bt__mesh__event__t" kindref="compound">wiced_bt_mesh_event_t</ref> *</type>
          <declname>p_event</declname>
        </param>
        <param>
